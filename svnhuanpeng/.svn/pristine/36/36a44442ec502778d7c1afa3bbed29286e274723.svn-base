@charset "UTF-8";
html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
font,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
figure,
figcaption {
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  font-weight: inherit;
  font-style: inherit;
  font-size: 100%;
  font-family: inherit;
  vertical-align: baseline;
}
a img,
iframe {
  border: none;
}
ol,
ul {
  list-style: none;
}
input,
textarea,
select,
button {
  font-size: 100%;
  font-family: inherit;
}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none !important;
  margin: 0;
}
html,
body {
  width: 100%;
  height: 100%;
  font-family: 微软雅黑;
  overflow-x: hidden;
  overflow-y: scroll;
  background: #f5f5f5;
  -webkit-touch-callout: none;
  -webkit-tap-highlight-color: transparent;
  -webkit-user-drag: none;
  -webkit-text-size-adjust: 100%;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-overflow-scrolling: touch;
  -webkit-text-size-adjust: 100%!important;
  -webkit-appearance: none;
  border: none;
  margin: 0;
  padding: 0;
}
#header {
  height: 3rem;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  z-index: 9999999;
  background: #fff;
}
#header a {
  font-size: 0;
  display: block;
}
#header img {
  width: 5.9375rem;
  height: 2.25rem;
  margin: 0.375rem 0.625rem;
}
.liveRoom-container {
  overflow-x: hidden;
  overflow-y: scroll;
  background-color: #f5f5f5;
  height: 100%;
}

.liveRoom-container .liveRoom-content #stage {
  background: #333;
  position: relative;
  overflow: hidden;
  height: 13.1875rem;
  margin-top: 3rem;
}
.liveRoom-container .liveRoom-content #stage .no-live {
  display: none;
  text-align: center;
}
.liveRoom-container .liveRoom-content #stage .no-live h1 {
  color: #fff;
  line-height: 5rem;
}
.liveRoom-container .liveRoom-content #stage .no-live h2 {
  color: #fff;
}
.liveRoom-container .liveRoom-content #stage .no-live a {
  text-decoration: none;
}
.liveRoom-container .liveRoom-content #stage .no-live a .btn_download_nolive {
  width: 10rem;
  height: 2rem;
  line-height: 2rem;
  margin: 2rem auto 0;
  background: #ff7800;
  text-align: center;
  color: #fff;
  border-radius: 0.3rem;
}
.liveRoom-container .liveRoom-content #stage .has-live {
  display: none;
  height: 13.1875rem;
}
.liveRoom-container .liveRoom-content #stage .has-live .btn_play_div {
  width: 100%;
  height: 13.1875rem;
  position: absolute;
  z-index: 999;
}
.liveRoom-container .liveRoom-content #stage .has-live .btn_play_div #btn_loading,
.liveRoom-container .liveRoom-content #stage .has-live .btn_play_div #btn_play {
  position: absolute;
  width: 3.125rem;
  height: 3.125rem;
  top: 50%;
  left: 50%;
  margin-top: -1.5625rem;
  margin-left: -1.5625rem;
  background: url("../img/icon_play.png") top center no-repeat;
  background-size: cover;
}
.liveRoom-container .liveRoom-content #stage .has-live .btn_play_div #btn_loading {
  color: #fff;
  font-size: .3rem;
  text-align: center;
  line-height: 3.125rem;
  display: none;
  background-image: url("../img/loadding.png");
}
.liveRoom-container .liveRoom-content #stage .has-live #poster {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 99;
  background: #000;
  text-align: center;
}
.liveRoom-container .liveRoom-content #stage .has-live #poster img {
  width: auto;
  height: 100%;
}
.liveRoom-container .liveRoom-content #stage #videoplayer {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 2;
}
.liveRoom-container .liveRoom-content #owner {
  height: 6.5625rem;
  background-color: #fff;
  margin-bottom: 0.1875rem;
}
.liveRoom-container .liveRoom-content #owner .row-1 {
  height: 4.25rem;
}
.liveRoom-container .liveRoom-content #owner .row-1:after {
  clear: both;
  display: table;
}
.liveRoom-container .liveRoom-content #owner .row-1 figure {
  width: 13.125rem;
  height: 3rem;
  float: left;
  margin: 0.625rem 0;
}
.liveRoom-container .liveRoom-content #owner .row-1 figure img {
  width: 2.5rem;
  height: 2.5rem;
  border-radius: 50%;
  float: left;
  margin: 0 0.625rem;
}
.liveRoom-container .liveRoom-content #owner .row-1 figure figcaption {
  float: left;
  width: 9.375rem;
  height: 3rem;
}
.liveRoom-container .liveRoom-content #owner .row-1 figure figcaption h2 {
  font-size: 1rem;
  color: #282828;
  font-weight: normal;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.liveRoom-container .liveRoom-content #owner .row-1 figure figcaption p {
  color: #929292;
  font-size: 0.8125rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.liveRoom-container .liveRoom-content #owner .row-1 figure figcaption p span {
  color: #ff7800;
}
.liveRoom-container .liveRoom-content #owner .row-1 .btn-toApp {
  float: right;
  width: 5.125rem;
  height: 1.875rem;
  line-height: 1.875rem;
  margin-top: 0.8125rem;
  margin-right: 0.625rem;
  text-align: center;
  font-size: 0.9375rem;
  background-color: #ff9b42;
  border-radius: 0.3125rem;
  text-align: center;
}
.liveRoom-container .liveRoom-content #owner .row-1 .btn-toApp > a button {
  text-align: center;
  background-color: transparent;
  margin: 0;
  padding: 0;
  color: #fff;
  outline: none;
  border: none;
  -webkit-appearance: none;
  line-height: 1.875rem;
}
.liveRoom-container .liveRoom-content #owner .row-2 {
  display: flex;
  box-sizing: content-box;
  padding: 0.625rem 0 0.9375rem;
  border-top: 1px solid whitesmoke;
  height: 1.5rem;
}
.liveRoom-container .liveRoom-content #owner .row-2 > div {
  flex-grow: 1;
  height: 1rem;
  border-right: 1px solid whitesmoke;
}
.liveRoom-container .liveRoom-content #owner .row-2 > div img {
  width: 1.75rem;
  height: 1.75rem;
  margin: -0.3125rem 0.375rem 0 1.25rem;
  float: left;
}
.liveRoom-container .liveRoom-content #owner .row-2 > div span {
  color: #929292;
  font-size: 0.75rem;
  float: left;
  display: inline-block;
  vertical-align: middle;
  margin-top: 0.0625rem;
}
.liveRoom-container .liveRoom-content #owner .row-2 > div:last-of-type {
  border-right: none;
}
/*.liveRoom-container .liveRoom-content #hot {*/
  /*height: 35rem;*/
/*}*/
.liveRoom-container .liveRoom-content #hot .hotLive {
  height: 2.5rem;
  overflow: hidden;
  margin-bottom: -0.5rem;
}
.liveRoom-container .liveRoom-content #hot .hotLive img {
  width: 2.0625rem;
  height: 1.5625rem;
  float: left;
  margin: 0.3125rem 0.5rem 0.375rem 0.625rem;
}
.liveRoom-container .liveRoom-content #hot .hotLive span {
  display: inline-block;
  vertical-align: middle;
  float: left;
  margin-top: 0.5rem;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul {
  padding: 0 0.375rem;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .more-div{
  height:4rem;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne {
  display: inline-block;
  width: 48%;
  height: 9.1875rem;
  position: relative;
  vertical-align: middle;
  margin: 0.5rem 1% 0;
  overflow: hidden;
  border-radius: 0.3125rem;
  background-color: #fff;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .div-poster {
  width: 100%;
  height: 6.0625rem;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  z-index: 2;
  background-color: rgba(0,0,0,0.8);
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .div-poster .img_poster {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  z-index: 1;
  /*width: 100%;*/
  height: 6.0625rem;
  margin: 0 auto;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .div-poster .img_title {
  width: 5rem;
  height: 1.125rem;
  text-align: center;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 3;
  background-color: rgba(0, 0, 0, 0.3);
  color: #ff993f;
  font-size: 0.8125rem;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .div-poster .img_author {
  width: 2.4375rem;
  height: 2.4375rem;
  border-radius: 50%;
  position: absolute;
  left: 0.25rem;
  bottom: -1.21875rem;
  background-color: #fff;
  z-index: 3;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .div-poster .img_author img {
  position: absolute;
  width: 2.25rem;
  height: 2.25rem;
  border-radius: 50%;
  left: 50%;
  top: 50%;
  margin-left: -1.125rem;
  margin-top: -1.125rem;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .author-desc {
  width: 100%;
  height: 1.25rem;
  position: absolute;
  left: 0;
  right: 0;
  top: 6.0625rem;
  z-index: 1;
  display: flex;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .author-desc:after {
  content: '';
  clear: both;
  display: table;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .author-desc p {
  height: 1.25rem;
  line-height: 1.25rem;
  font-size: 0.75rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .author-desc .author-name {
  float: left;
  width: 70%;
  text-indent: 3.125rem;
  color: #565656;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .author-desc .author-person {
  float: right;
  width: 30%;
  text-align: right;
  margin-right: 0.5rem;
  font-size: 0.75rem;
  color: #929292;
}
.liveRoom-container .liveRoom-content #hot #recommendlist ul .liveOne .room-name {
  width: 100%;
  height: 1.5625rem;
  line-height: 1.5625rem;
  position: absolute;
  font-size: 0.875rem;
  color: #282828;
  text-indent: 0.5rem;
  text-align: left;
  left: 0;
  right: 0;
  bottom: 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.hp_download {
  position: fixed;
  z-index: 99999;
  left: 0;
  right: 0;
  bottom: 0;
  height: 3.125rem;
  background-color: #282828;
  opacity: 0.88;
  font-family: 微软雅黑;
}
.hp_download .close {
  float: left;
  width: 1.25rem;
  height: 1.25rem;
  padding: 0.9375rem 0.1875rem;
  box-sizing: content-box;
}
.hp_download figure {
  margin: 0;
  padding-top: 0.3125rem;
  margin-left: 0.1875rem;
  float: left;
}
.hp_download figure img {
  float: left;
  width: 2.5rem;
  height: 2.5rem;
  border-radius: 0.3125rem;
}
.hp_download figure figcaption {
  float: left;
  margin-left: 0.5rem;
  margin-top: -0.25rem;
}
.hp_download figure figcaption p {
  color: #ff7800;
  font-size: 1.0625rem;
  letter-spacing: 0.03125rem;
  margin: 0.125rem 0 -0.125rem;
}
.hp_download figure figcaption span {
  font-size: 0.8125rem;
  color: #fff;
}
.hp_download .openApp {
  float: right;
  color: #fff;
  font-size: 1rem;
  background-color: #ff7800;
  border: none;
  padding: 0.4rem 1rem;
  margin-top: 0.45rem;
  margin-right: 0.5rem;
  text-decoration: none;
  border-radius: 0.3125rem;
}
