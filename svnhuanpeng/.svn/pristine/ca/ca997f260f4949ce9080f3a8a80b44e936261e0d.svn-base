html .request-container .request-listModal,
body .request-container .request-listModal {
  display: none;
  box-sizing: border-box;
  width: 20rem;
  padding:3.125rem 0 0 0.75rem;
  background: url("../img/getAwardTitle.png") 50% 0.5rem / 50% no-repeat;
  background-color: #fff8e5;
  border-radius: 0.3125rem;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -10rem;
  margin-top: -8.625rem;
  z-index: 999;
  -ms-animation: run 1s linear;
  -moz-animation: run 1s linear;
  -o-animation: run 1s linear;
  -webkit-animation: run 1s linear;
  animation: run 1s linear;
}
@keyframes run {
  0% {
    transform: scale(1);
  }
  50% {
    transform: scale(1.05);
  }
  100% {
    transform: scale(1);
  }
}
