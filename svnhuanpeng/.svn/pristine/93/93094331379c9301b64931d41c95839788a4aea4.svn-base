  white-space: nowrap;
  text-overflow: ellipsis;
  clear: both;
}
.liveroom-chat .commend-selbox .poster-info {
  font-size: 0.75rem;
  margin-top: 0.375rem;
}
.liveroom-chat .commend-selbox .poster-info .poster-name {
  width: 70%;
  float: left;
  text-indent: 3.125rem;
  color: #565656;
}
.liveroom-chat .commend-selbox .poster-info .audience {
  width: 30%;
  float: right;
  text-align: right;
  padding-right: 0.5rem;
  box-sizing: border-box;
  color: #929292;
}
.liveroom-chat .commend-selbox .poster-info p {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.liveroom-chat .anchor-info {
  height: 3.5rem;
  padding: 0.625rem 0;
  background: #fff;
  position: relative;
  -webkit-transition: all .2s linear;
  transition: all .2s linear;
}
.liveroom-chat .anchor-info .room-info {
  padding: 0 0.625rem;
