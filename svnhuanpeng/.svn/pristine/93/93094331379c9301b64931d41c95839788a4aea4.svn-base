  margin-left: 0.78125rem;
  background: url("../images/icon_refresh.png") center center no-repeat;
  background-size: 60%;
}
.liveroom-content .video-bar .video-pause {
  float: left;
}
.liveroom-content .video-bar .video-pause.normal {
  background: url("../images/icon_pause.png") no-repeat;
  background-size: cover;
}
.liveroom-content .video-bar .video-pause.high {
  background: url("../images/icon_pause_h.png") no-repeat;
  background-size: cover;
}
.liveroom-content .video-bar .video-full {
  float: right;
}
.liveroom-content .video-bar .video-full.normal {
  background: url("../images/icon_full.png") no-repeat;
  background-size: cover;
}
.liveroom-content .video-bar .video-full.high {
  background: url("../images/icon_full_h.png") no-repeat;
  background-size: cover;
}
.liveroom-content .video-bar.show {
  -webkit-transform: translateY(-2.5rem);
  transform: translateY(-2.5rem);
}
.liveroom-content .poster {
  position: absolute;
  z-index: 4;
  left: 0;
  right: 0;
  top: 0;
  width: 100%;
  height: 100%;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
}
.liveroom-content .poster.filter {
  -webkit-filter: blur(0.40625rem);
  filter: blur(0.40625rem);
}
.liveroom-content .poster.landscape {
  width: 100%;
  height: 100%;
}
.liveroom-content .poster.portrait {
  width: 100%;
  height: auto;
  top: -40vh;
}
.liveroom-content .play-btn {
  display: none;
  width: 4rem;
  height: 4rem;
  position: absolute;
  z-index: 5;
  left: 50%;
  top: 50%;
  margin-left: -2rem;
  margin-top: -2rem;
  border: none;
  outline: none;
  border-radius: 50%;
}
.liveroom-content .play-btn.normal {
  background: url("../images/play.png") no-repeat;
  background-size: cover;
}
.liveroom-content .play-btn.high {
  background: url("../images/play_h.png") no-repeat;
  background-size: cover;
}
.liveroom-content .nolive {
  display: none;
  width: 9.625rem;
  height: 5.625rem;
  position: absolute;
  z-index: 6;
  left: 50%;
  top: 50%;
  margin-left: -4.8125rem;
  margin-top: -2.8125rem;
  border: none;
  outline: none;
  background: url("../images/icon_nolive.png") no-repeat;
  background-size: cover;
}
.liveroom-chat .sec-box {
  height: 2.75rem;
  background-color: #fff;
  border-bottom: 1px solid #dfdfdf;
}
.liveroom-chat .sec-box ul {
  display: flex;
}
.liveroom-chat .sec-box li {
