}
@-webkit-keyframes fontSize {
  from {
    -webkit-transform: scale(1.4);
    transform: scale(1.4);
  }
  to {
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}
@keyframes fontSize {
  from {
    transform: scale(1.4);
  }
  to {
    transform: scale(1);
  }
}
.liveroom-chat .chat-container .chat-cell .message {
  display: inline;
  margin-left: 0.25rem;
  color: #787878;
  font-size: 0.9375rem;
  line-height: 1.6;
}
.liveroom-chat .chat-container .chat-cell .message.gift {
