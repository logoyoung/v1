  height: 100%;
  display: flex;
  align-content: flex-start;
  flex-wrap: wrap;
}
.chat-box .gift-content ul > li {
  width: 25%;
  height: 4.375rem;
  text-align: center;
  display: inline-block;
  margin: 0.75rem 0 0.625rem 0;
}
.chat-box .gift-content ul > li .img {
  width: 3rem;
  height: 3rem;
  margin: 0 auto;
  border-radius: 0.46875rem;
}
.chat-box .gift-content ul > li .img img {
  width: 100%;
  height: 100%;
  border-radius: 0.46875rem;
}
.chat-box .gift-content ul > li p {
  width: 100%;
  height: 1rem;
  margin: 0.09375rem 0;
}
.chat-box .gift-content ul > li .desc {
  font-size: 0.875rem;
  color: #fff;
}
.chat-box .gift-content ul > li .price {
  font-size: 0.75rem;
  color: rgba(255, 255, 255, 0.5);
}
.chat-box .gift-content .userMoney {
  display: none;
  height: 0;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
  margin: 0.3125rem 0 0 0;
  padding: 0 1.25rem;
  font-size: 0.875rem;
  color: #fff;
}
.chat-box .gift-content .userMoney > div {
  width: 40%;
  height: 100%;
}
.chat-box .gift-content .userMoney > div:last-of-type {
  width: 20%;
  text-align: right;
}
.chat-box .gift-content .userMoney span {
  display: inline-block;
  vertical-align: middle;
  height: 1.5rem;
  line-height: 1.5rem;
}
.chat-box .gift-content .userMoney .coin {
  width: 1.5rem;
  height: 1.5rem;
  background: url("../images/icon_huanpeng_coin.png") center center no-repeat;
  background-size: cover;
}
.chat-box .gift-content .userMoney .coinMoney,
.chat-box .gift-content .userMoney .beanMoney {
  width: 65%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.chat-box .gift-content .userMoney .bean {
  width: 1.5rem;
  height: 1.5rem;
  background: url("../images/icon_huanpeng_bean.png") center center no-repeat;
  background-size: cover;
}
.chat-box .gift-content .userMoney .recharge {
  width: 3rem;
  height: 1.5rem;
  padding: 0 0.15625rem;
  border-radius: 0.25rem;
  text-align: center;
  background-color: #ff7800;
  border: none;
  outline: none;
  -webkit-appearance: none;
  color: #fff;
  font-size: 0.875rem;
}
.chat-box .gift-content .userMoney .recharge a {
  color: #fff;
}
.chat-box .gift-content .userMoney.show {
  display: flex;
  height: 1.5rem;
}
.chat-box .gift-content.show {
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
  -webkit-transform: translateY(0);
  transform: translateY(0);
}
.chat-box .emoji-btn,
.chat-box .send-btn {
  display: none;
}
.chat-box.int {
  height: auto;
  padding: 0;
}
.chat-box.int .user-tools {
  border-top: 1px solid #ccc;
  background-color: #fff;
}
.chat-box.int .user-tools .gift-btn,
.chat-box.int .user-tools .bean-btn {
  display: none;
}
.chat-box.int .user-tools .emoji-btn,
.chat-box.int .user-tools .send-btn {
  display: block;
}
.chat-box.int .user-tools .chat-btn {
  width: auto;
  height: 2.25rem;
  margin: 0 4rem 0 2.25rem;
  border-radius: 0.375rem;
  overflow: hidden;
  position: relative;
  float: none;
  background: none;
}
.chat-box.int .user-tools .chat-btn .input {
  width: 100%;
  height: 100%;
  position: absolute;
  border-radius: 0.375rem;
  font-size: 0.9375rem;
  text-indent: 0.5rem;
  background-image: none;
  background: #f0f0f0;
  color: #282828;
  -webkit-transition: all .1s linear;
  transition: all .1s linear;
}
.chat-box.int .user-tools .chat-btn .input::-webkit-input-placeholder {
  color: #ccc;
  line-height: 1.2;
}
.chat-box.int .emoji-content.show {
  height: 12.5rem;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
}
.chat-box.none {
  display: none;
}
