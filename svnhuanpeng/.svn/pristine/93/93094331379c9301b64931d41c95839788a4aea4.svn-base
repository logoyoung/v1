  word-break: break-all;
  font-size: 0;
}
.liveroom-chat .chat-container .chat-cell span {
  display: inline-block;
  vertical-align: middle;
  margin: 0 0.09375rem;
}
.liveroom-chat .chat-container .chat-cell .text {
