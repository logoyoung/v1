  height: 1rem;
}
.liveroom-chat .anchor-info .room-info figcaption .anchor-desc .room-title {
  width: 12.5rem;
  font-size: 0.875rem;
  color: #282828;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  -webkit-user-select: none;
  user-select: none;
}
.liveroom-chat .anchor-info .room-info figcaption .anchor-game i {
  font-style: normal;
  color: #929292;
  font-size: 0.75rem;
}
.liveroom-chat .anchor-info .room-info figcaption .anchor-game .room-userNum {
  font-size: 0.75rem;
  color: #929292;
}
.liveroom-chat .anchor-info .room-info figcaption .anchor-game .room-game {
  color: #ff993f;
  font-size: 0.75rem;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 0.375rem;
}
.liveroom-chat .anchor-info .follow-btn {
  width: 3.9375rem;
  height: 1.75rem;
  position: absolute;
  right: 0.625rem;
  top: 50%;
  margin-top: -0.875rem;
  font-size: 0.875rem;
  border-radius: 1.5625rem;
  background-color: #ff7800;
  color: #fff;
  border: none;
  outline: none;
  -webkit-user-select: none;
  user-select: none;
  padding: 0;
}
.liveroom-chat .anchor-info .follow-btn.cancel {
  background-color: #cccccc;
}
.liveroom-chat .anchor-info .anchor-display {
  width: 3.25rem;
  height: 1.25rem;
  position: absolute;
  z-index: 2;
  left: 50%;
  margin-left: -1.640625rem;
  bottom: -1.21875rem;
  -webkit-transition: all .2s linear;
  transition: all .2s linear;
}
.liveroom-chat .anchor-info .anchor-display.show {
  background: url("../images/icon_show.png") no-repeat;
  background-size: cover;
}
.liveroom-chat .anchor-info .anchor-display.show-high {
  background: url("../images/icon_show_h.png") no-repeat;
  background-size: cover;
}
.liveroom-chat .anchor-info .anchor-display.hide {
  background: url("../images/icon_hide.png") no-repeat;
  background-size: cover;
}
.liveroom-chat .anchor-info .anchor-display.hide-high {
  background: url("../images/icon_hide_h.png") no-repeat;
  background-size: cover;
}
.liveroom-chat .anchor-info.none {
  -webkit-transition: all .2s linear;
  transition: all .2s linear;
  padding: 0;
  height: 0;
}
.liveroom-chat .anchor-info.none .room-info,
.liveroom-chat .anchor-info.none .follow-btn {
  display: none;
}
.liveroom-chat .chat-container {
  width: 100%;
  height: 15.625rem;
  background-color: #f5f5f5;
  overflow-x: hidden;
  overflow-y: scroll;
  padding: 0.3125rem 0.625rem 5.625rem 0.625rem;
  z-index: 1;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
}
.liveroom-chat .chat-container .chat-div {
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
}
.liveroom-chat .chat-container .chat-cell {
