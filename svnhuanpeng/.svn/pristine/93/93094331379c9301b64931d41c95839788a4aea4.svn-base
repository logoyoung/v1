@charset "UTF-8";
html,
body {
  min-width: 20rem;
  overflow: hidden;
  font-family: 'PingFang SC', 'STHeitiSC-Light', 'Helvetica-Light', arial, sans-serif;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
  position: relative;
}
.body.int {
  top: -6vh;
}
.mask,
.modal {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 99;
  background-color: rgba(0, 0, 0, 0.5);
  display: none;
}
.modal {
  background: transparent;
}
.mobile-tip {
  display: none;
  width: 9.375rem;
  height: 7rem;
  position: absolute;
  z-index: 99999;
  left: 50%;
  top: 50%;
  margin-left: -4.6875rem;
  margin-top: -4.6875rem;
  background: rgba(0, 0, 0, 0.4);
  text-align: center;
  border-radius: 0.46875rem;
}
.mobile-tip > div {
  width: 100%;
  height: 100%;
  border-radius: 0.46875rem;
  padding-top: 1.25rem;
}
.mobile-tip > div img {
  width: 2.5rem;
  height: 2.5rem;
}
.mobile-tip > div p {
  margin-top: 0.625rem;
  font-size: 0.9375rem;
  word-break: break-all;
  line-height: 1.2;
  width: 100%;
  text-align: center;
  color: #fff;
}
.mobile-tip > div span {
  display: inline;
  color: #ff7800;
}
.liveroom-container {
  width: 100%;
  height: 100%;
  position: relative;
}
header {
  width: 100%;
  height: 2.75rem;
  line-height: 2.75rem;
  font-size: 1.25rem;
  color: #282828;
  background: #fff;
  text-align: center;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  padding: 0 15%;
  box-shadow: 0 2px 0 rgba(22, 22, 22, 0.3);
  position: relative;
}
header .back {
  width: 1.75rem;
  height: 1.75rem;
  position: absolute;
  top: 0.5rem;
  left: 0.5rem;
}
header .back.normal {
  background: url("../images/icon_back.png") center center no-repeat;
  background-size: cover;
}
header .back.high {
  background: url("../images/icon_back_h.png") center center no-repeat;
  background-size: cover;
}
.liveroom-content {
  width: 100%;
  height: 13.1875rem;
  position: relative;
  overflow: hidden;
  background: black;
  text-align: center;
}
.liveroom-content .video,
.liveroom-content .my-canvas {
  height: 0;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  -webkit-transition: all .1s linear;
  transition: all .1s linear;
}
.liveroom-content video:-webkit-full-screen {
  width: 100%;
  height: 100%;
  max-height: 100%;
}
.liveroom-content .video {
  z-index: 1;
}
.liveroom-content .video.play {
  -webkit-transition: all .1s linear;
  transition: all .1s linear;
  width: 100%;
  height: 100%;
}
.liveroom-content .my-canvas {
  z-index: 2;
}
.liveroom-content .my-canvas canvas {
  width: 100%;
  height: 100%;
}
.liveroom-content .video-bar {
  position: absolute;
  left: 0;
  right: 0;
  width: 100%;
  height: 2.5rem;
  padding: 0.125rem 0.1875rem;
