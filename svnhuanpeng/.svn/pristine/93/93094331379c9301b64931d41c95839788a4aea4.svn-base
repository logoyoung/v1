  background-color: #ffffff;
}
.liveroom-chat .commend-selbox .poster {
  width: 100%;
  height: 100%;
}
.liveroom-chat .commend-selbox .poster .snapshot {
  position: relative;
  width: 100%;
  height: 6.0625rem;
  overflow: hidden;
  border-top-left-radius: 0.3125rem;
  border-top-right-radius: 0.3125rem;
}
.liveroom-chat .commend-selbox .poster .snapshot .horizontal {
  display: block;
