  line-height: 1.0625rem;
  font-size: 0.875rem;
}
.liveroom-chat .anchor-info .room-info figcaption p span {
  display: inline-block;
  vertical-align: middle;
}
.liveroom-chat .anchor-info .room-info figcaption .anchor-desc {
  margin-bottom: 0.1875rem;
}
.liveroom-chat .anchor-info .room-info figcaption .anchor-desc .ms-lv {
