@charset "UTF-8";
@rem: 32rem;
@-webkit-keyframes breath1{
  0%{
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 0.4;
  }
  50%{
    -webkit-transform: scale(1.05);
    transform: scale(1.05);
    opacity: 0.2;
  }
  100%{
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 0.4;
  }
}
@keyframes breath1{
  0%{
    transform: scale(0.95);
    opacity: 0.4;
  }
  50%{
    transform: scale(1.05);
    opacity: 0.2;
  }
  100%{
    transform: scale(0.95);
    opacity: 0.4;
  }
}
@-webkit-keyframes breath2{
  0%{
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 0.6;
  }
  50%{
    -webkit-transform: scale(1.05);
    transform: scale(1.05);
    opacity: 0.1;
  }
  100%{
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 0.6;
  }
}
@keyframes breath2{
  0%{
    transform: scale(0.95);
    opacity: 0.6;
  }
  50%{
    transform: scale(1.05);
    opacity: 0.4;
  }
  100%{
    transform: scale(0.95);
    opacity: 0.6;
  }
}
@-webkit-keyframes breath3{
  0%{
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 1;
  }
  50%{
    -webkit-transform: scale(1.05);
    transform: scale(1.05);
    opacity: 0.8;
  }
  100%{
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 1;
  }
}
@keyframes breath3{
  0%{
    transform: scale(0.95);
    opacity: 1;
  }
  50%{
    transform: scale(1.05);
    opacity: 0.8;
  }
  100%{
    transform: scale(0.95);
    opacity: 1;
  }
}

@-webkit-keyframes breath{
  0%{
    -webkit-transform: scale(1.05);
    transform: scale(1.05);
  }
  50%{
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
  }
  100%{
    -webkit-transform: scale(1.05);
    transform: scale(1.05);
  }
}
@keyframes breath{
  0%{
    transform: scale(1.05);
  }
  50%{
    transform: scale(0.95);
  }
  100%{
    transform: scale(1.05);
  }
}
@keyframes run{
  0%{
    transform: translateY(-2px);
  }
  50%{
    transform: translateY(2px);
  }
  100%{
    transform: translateY(-2px);
  }
}
@-webkit-keyframes run{
  0%{
    -webkit-transform: translateY(-2px);
    transform: translateY(-2px);
  }
  50%{
    -webkit-transform: translateY(2px);
    transform: translateY(2px);
  }
  100%{
    -webkit-transform: translateY(-2px);
    transform: translateY(-2px);
  }
}
html,body{
  width: 100%;
  min-width: 320px;
  height: 100%;
  overflow-x: hidden;
  overflow-y: scroll;
  font-family: 微软雅黑;
  //position: relative;
  -webkit-overflow-scrolling: touch;
  overflow-scrolling: touch;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
}
.video-center{
  position: absolute;
  background: url("../img/video-center.png") center center no-repeat;
  background-size: cover;
  z-index: 999999;
  -webkit-transition: all 200ms linear;
  transition: all 200ms linear;
}
.video-center.loadVideo{
  width: 500/@rem;
  height: 500/@rem;
  right: 50%;
  top: 50%;
  margin-right: -250/@rem;
  margin-top: -500/@rem;
}
.video-center.normal{
  width: 300/@rem;
  height: 300/@rem;
  right: 0/@rem;
  top: 0/@rem;
  margin: 0;
}
.video-touch{
  position: absolute;
  width: 100/@rem;
  height: 100/@rem;
  border-radius: 50%;
  z-index: 999999;
  right: 50%;
  margin-right: -50/@rem;
  bottom: 40/@rem;
  .touch-third,.touch-two,.touch-first{
    width: 100%;
    height: 100%;
    border-radius: 50%;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
  }
  .touch-third{
    z-index: 1;
    background: url("../img/touch-3.png") no-repeat;
    background-size: cover;
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 0.4;
    -webkit-animation: breath1 infinite 3000ms ease-in-out;
    animation: breath1 infinite 3000ms ease-in-out;
    animation-delay: 1250ms;
  }
  .touch-two{
    z-index: 3;
    background: url("../img/touch-2.png") no-repeat;
    background-size: cover;
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 1;
    //-webkit-animation: breath3 infinite 4500ms linear;
    //animation: breath3 infinite 4500ms linear;
  }
  .touch-first{
    z-index: 5;
    background: url("../img/touch-1.png") no-repeat;
    background-size: cover;
    -webkit-transform: scale(0.95);
    transform: scale(0.95);
    opacity: 0.6;
    -webkit-animation: breath2 infinite 3000ms ease-in-out;
    animation: breath2 infinite 3000ms ease-in-out;
    animation-delay: 750ms;
  }
}
.mask-container{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 99999;
  background-color: rgba(0,0,0,0.4);
}
.share-container{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 9998;
  overflow: hidden;
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
  .video-content{
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 1;
  }
  .video-swipeTop{
    width: 160/@rem;
    height: 90/@rem;
    position: absolute;
    bottom: 10/@rem;
    left: 50%;
    margin-left: -80/@rem;
    z-index: 5;
    font-size: 26/@rem;
    color: #fff;
    line-height: 1.4;
    text-align: center;
    background: url("../img/swipeTop.png") top center no-repeat;
    background-size: 44/@rem 44/@rem;
    padding-top: 50/@rem;
    -webkit-animation: run linear 1500ms infinite;
    animation: run linear 1500ms infinite;
  }
}
.coupon-container{
  width: 100%;
  height: 100%;
  -webkit-overflow-scrolling : touch;
  overflow-y: scroll;
  background-color: rgb(108, 113, 248);
  -webkit-transition: all .3s linear;
  transition: all .3s linear;
  .background-top{
    height: 602/@rem;
    background: url("../img/background-top.png") center center no-repeat;
    background-size: cover;
  }
  .coupon-content{
    .none{
      display: none;
    }
    .coupon-first{
      padding: 0 128/@rem;
      section{
        width: 100%;
        height: 68/@rem;
        margin: 28/@rem auto;
        overflow: hidden;
        border-radius: 10/@rem;
        position: relative;
      }
      section:nth-of-type(1){
        background-color: #fff;
      }
      section:nth-of-type(2){
        background-color: #ff7800;
      }
      .input{
        position: absolute;
        width: 100%;
        height: 45/@rem;
        top: 50%;
        margin-top: -22.5/@rem;
        line-height: 45/@rem;
        border: none;
        outline: none;
        font-size: 34/@rem;
        color: #282828;
        text-indent: 85/@rem;
        &::-webkit-input-placeholder{
          color: #ccc;
          font-size: 34/@rem;
          text-indent: 100/@rem;
        }
      }
      .button{
        position: absolute;
        font-size: 34/@rem;
        letter-spacing: 2/@rem;
        width: 100%;
        height: 100%;
        border: none;
        outline: none;
        background-color: #ff7800;
        color: #fff;
        -webkit-appearance: none;
      }
    }
    .coupon-second{
      width: 448/@rem;
      height: 236/@rem;
      margin: 0 auto;
      background: url("../img/coupon-bg.png") no-repeat;
      background-size: cover;
      position: relative;
      section{
        position: absolute;
        width: 420/@rem;
        height: 188/@rem;
        top: 50%;
        left: 50%;
        margin-left: -210/@rem;
        margin-top: -94/@rem;
      }
      .coupon-receive{
        z-index: 5;
        button{
          position: absolute;
          width: 220/@rem;
          height: 76/@rem;
          left: 50%;
          top: 50%;
          margin-left: -110/@rem;
          margin-top: -38/@rem;
          color: #fff;
          font-weight: 500;
          letter-spacing: 2/@rem;
          border-radius: 10/@rem;
          background-color: #ff7800;
          border: none;
          outline: none;
          -webkit-appearance: none;
        }
      }
      .canvas{
        z-index: 3;
        overflow: hidden;
      }
      .canvas.hide{
        background-color: #ccc;
      }
      .info{
        z-index: 1;
        p{
          text-align: center;
          line-height: 1.3;
        }
        .desc{
          color: #666;
          font-size: 32/@rem;
          margin: 35/@rem 0 0 0;
        }
        .money{
          color: #ff7800;
          font-size: 42/@rem;
          span{
            display: inline-block;
            font-weight: 500;
          }
          .num{
            font-size: 60/@rem;

            transform: translateY(2px);
          }
        }
        .fail{
          color: #bd4b00;
          font-size: 32/@rem;
          &:first-of-type{
            margin: 40/@rem 0 20/@rem 0;
          }
        }
      }
    }
  }
  .coupon-desc{
    height: 250/@rem;
    p{
      font-size: 24/@rem;
      color: #fff;
      text-align: center;
      line-height: 40/@rem;
    }
    padding: 20/@rem 0 50/@rem 0;
  }
  .coupon-content{
    position: relative;
    z-index: 1;
    height: 210/@rem;
    margin-bottom: 45/@rem;
  }
  .video-open{
    position: absolute;
    width: 80/@rem;
    height: 80/@rem;
    background: url("../img/video-btn.png") no-repeat;
    background-size: cover;
    right: 20/@rem;
    top: 20/@rem;
    z-index: 99;
    -webkit-animation: breath infinite 2000ms ease-in;
    animation: breath infinite 2000ms ease-in;
  }
}
.coupon-container.focus{
  -webkit-transform:translateY(-50vh);
  transform:translateY(-50vh);
}
.hp_download{
  position: absolute;
  z-index: 99;
  left: 0;
  right: 0;
  bottom: -2/@rem;
  height: 102/@rem;
  background-color: #282828;
  opacity: 0.88;
  .close{
    float: left;
    width: 1.25rem;
    height: 1.25rem;
    padding: 0.9375rem 0.1875rem;
    box-sizing: content-box;
  }
  figure {
    margin: 0 0 0 0.1875rem;
    padding-top: 0.3125rem;
    float: left;
    img {
      float: left;
      width: 2.5rem;
      height: 2.5rem;
      border-radius: 0.3125rem;
    }
  }
  figcaption {
    float: left;
    margin-left: 0.5rem;
    margin-top: -0.25rem;
    p {
      color: #ff7800;
      font-size: 1.0625rem;
      letter-spacing: 0.03125rem;
      margin: 0.125rem 0 0 0;
    }
    span {
      font-size: 0.8125rem;
      color: #fff;
      display: inline-block;
      margin-top: 1/@rem;
      vertical-align: middle;
    }
  }
  .openApp {
    float: right;
    color: #fff;
    font-size: 1rem;
    background-color: #ff7800;
    border: none;
    padding: 0.4rem 1rem;
    margin-top: 0.45rem;
    margin-right: 0.5rem;
    text-decoration: none;
    border-radius: 0.3125rem;
  }
}
.modal-box{
  display: none;
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  .error-modal{
    display: none;
    position: absolute;
    width: 10rem;
    height: 7.5rem;
    left: 50%;
    top: 50%;
    z-index: 99999;
    margin-top: -6.5625rem;
    margin-left: -5rem;
    background-color: #fff;
    border-radius: 0.5rem;
    border: 1px solid whitesmoke;
    img{
      width: 2.25rem;
      height: 2.25rem;
      position: absolute;
      z-index: 999999;
      left: 50%;
      top: 50%;
      margin-left: -1.125rem;
      margin-top: -2.25rem;
    }
    p{
      position: absolute;
      z-index: 99999;
      left: 0;
      right: 0;
      top: 50%;
      margin-top: 0.75rem;
      width: 100%;
      height: 1rem;
      font-size: 0.8125rem;
      text-align: center;
      word-break: break-all;
    }
  }
}
