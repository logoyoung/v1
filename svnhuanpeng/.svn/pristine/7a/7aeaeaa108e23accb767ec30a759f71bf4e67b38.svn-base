@charset "UTF-8";
html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
font,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
figure,
figcaption {
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  font-weight: inherit;
  font-style: inherit;
  font-size: 100%;
  font-family: inherit;
  vertical-align: baseline;
}
a img,
iframe {
  border: none;
}
ol,
ul {
  list-style: none;
}
input,
textarea,
select,
button {
  font-size: 100%;
  font-family: inherit;
}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none !important;
  margin: 0;
}
html,
body {
  width: 100%;
  height: 100%;
  font-family: 微软雅黑;
  overflow-x: hidden;
  background: #f5f5f5;
  -webkit-tap-highlight-color: transparent;
  -webkit-user-drag: none;
  -webkit-text-size-adjust: 100%;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-text-size-adjust: 100%!important;
  -webkit-appearance: none;
  border: none;
  margin: 0;
  padding: 0;
  overflow-y: scroll;
}
.clickMore{
  text-align: center;
  height: 4rem;
  line-height: 4rem;
  position: relative;
}
#moreBtn,#endBtn{
  width: 7rem;
  height: 2rem;
  line-height: 2rem;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -3.5rem;
  margin-top: -1rem;
  border: 1px solid #565656;
  border-radius: 6rem;
  color: #565656;
  font-size: 0.8rem;
  text-align: center;
}
.loadingMask{
  position: fixed;
  z-index: 999999;
  width: 100%;
  height: 100%;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: url('../img/splash.png') top center no-repeat;
  background-size: cover;
}
.loading{
  width: 100%;
  height: 3rem;
  position: absolute;
  left: 0;
  right: 0;
  top: 50%;
  margin-top: 5.5rem;
  text-align: center;
}
.spinner,.clickMore .moreStatus{
  margin: 0 auto;
  width: 1.6rem;
  height: 1.6rem;
  position: relative;
  text-align: center;
  -webkit-animation: rotate 2.0s infinite linear;
  animation: rotate 2.0s infinite linear;
}
.clickMore .moreStatus{
  margin-top: 1rem;
}

.dot1, .dot2 {
  width: 60%;
  height: 60%;
  display: inline-block;
  position: absolute;
  top: 0;
  background-color: #ff7800;
  border-radius: 100%;

  -webkit-animation: bounce 2.0s infinite ease-in-out;
  animation: bounce 2.0s infinite ease-in-out;
}

.dot2 {
  top: auto;
  bottom: 0;
  -webkit-animation-delay: -1.0s;
  animation-delay: -1.0s;
}

@-webkit-keyframes rotate { 100% { -webkit-transform: rotate(360deg); }}
@keyframes rotate { 100% { transform: rotate(360deg) ; -webkit-transform: rotate(360deg) ; }}

@-webkit-keyframes bounce {
  0%, 100% { -webkit-transform: scale(0.0) }
  50% { -webkit-transform: scale(1.0) }
}

@keyframes bounce {
  0%, 100% {
    transform: scale(0.0);
    -webkit-transform: scale(0.0);
  } 50% {
      transform: scale(1.0);
      -webkit-transform: scale(1.0);
    }
}
#header {
  height: 3rem;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  z-index: 50;
  background-color: #fff;
  box-shadow: 0 1px 0 rgba(22, 22, 22, 0.3);
}
#header a {
  font-size: 0;
  display: block;
}
#header img {
  width: 5.9375rem;
  height: 2.25rem;
  margin: 0.375rem 0.625rem;
}
#hp-app-container {
  -webkit-touch-callout: none;
  -webkit-overflow-scrolling: touch;
  overflow-x: hidden;
  overflow-y: scroll;
  height: 100%;
}
#hp-app-container .home-app .layout-banner {
  width: 100%;
  height: 6rem;
  margin-top: 3rem;
}
#hp-app-container .home-app .layout-banner .swiper-container {
  height: 100%;
}
#hp-app-container .home-app .layout-banner .swiper-container .banPic {
  width: 100%;
  height: 100%;
}
#hp-app-container .home-app .layout-banner .swiper-container .swiper-pagination {
  bottom: 0.125rem;
}
#hp-app-container .home-app .layout-banner .swiper-container .swiper-pagination-bullet {
  background-color: rgba(0, 0, 0, 0.8);
}
#hp-app-container .home-app .layout-banner .swiper-container .swiper-pagination-bullet-active {
  background-color: #fff;
}
#hp-app-container .home-app .layout-banner .swiper-container .swiper-slide a {
  height: 6rem;
  display: block;
}
#hp-app-container .home-app #hot .hotLive {
  height: 2.1875rem;
  margin-bottom: -0.3125rem;
  overflow: hidden;
}
#hp-app-container .home-app #hot .hotLive img {
  width: 2.0625rem;
  height: 1.5625rem;
  float: left;
  margin: 0.3125rem 0.5rem 0.375rem 0.625rem;
}
#hp-app-container .home-app #hot .hotLive span {
  display: inline-block;
  vertical-align: middle;
  float: left;
  margin-top: 0.5rem;
}
#hp-app-container .home-app #hot #recommendlist ul {
  padding: 0 0.375rem;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne {
  display: inline-block;
  width: 48%;
  height: 9.1875rem;
  position: relative;
  vertical-align: middle;
  margin: 0.5rem 1% 0;
  overflow: hidden;
  border-radius: 0.3125rem;
  background-color: #fff;
}

#hp-app-container .home-app #hot #recommendlist ul .more-div{
  height: 3.4375rem;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .div-poster {
  width: 100%;
  height: 6.0625rem;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  z-index: 2;
  background-color: #000;
  text-align: center;
  border-top-left-radius: 0.3125rem;
  border-top-right-radius: 0.3125rem;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .div-poster .img_poster {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  z-index: 1;
  height: 100%;
  margin: 0 auto;
  border-top-left-radius: 0.3125rem;
  border-top-right-radius: 0.3125rem;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .div-poster .img_title {
  width: 5rem;
  height: 1.125rem;
  text-align: center;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 3;
  background-color: rgba(0, 0, 0, 0.3);
  color: #ff993f;
  font-size: 0.8125rem;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .div-poster .img_author {
  width: 2.4375rem;
  height: 2.4375rem;
  border-radius: 50%;
  position: absolute;
  left: 0.25rem;
  bottom: -1.21875rem;
  background-color: #fff;
  z-index: 3;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .div-poster .img_author img {
  position: absolute;
  width: 2.25rem;
  height: 2.25rem;
  border-radius: 50%;
  left: 50%;
  top: 50%;
  margin-left: -1.125rem;
  margin-top: -1.125rem;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .author-desc {
  width: 100%;
  height: 1.25rem;
  position: absolute;
  left: 0;
  right: 0;
  top: 6.0625rem;
  z-index: 1;
  display: flex;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .author-desc:after {
  content: '';
  clear: both;
  display: table;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .author-desc p {
  height: 1.25rem;
  line-height: 1.25rem;
  font-size: 0.75rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .author-desc .author-name {
  float: left;
  width: 70%;
  text-indent: 3.125rem;
  color: #565656;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .author-desc .author-person {
  float: right;
  width: 30%;
  text-align: right;
  margin-right: 0.5rem;
  font-size: 0.75rem;
  color: #929292;
}
#hp-app-container .home-app #hot #recommendlist ul .liveOne .room-name {
  width: 100%;
  height: 1.5625rem;
  line-height: 1.5625rem;
  position: absolute;
  font-size: 0.875rem;
  color: #282828;
  text-indent: 0.5rem;
  text-align: left;
  left: 0;
  right: 0;
  bottom: 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.hp_download {
  position: fixed;
  z-index: 99999;
  left: 0;
  right: 0;
  bottom: 0;
  height: 3.125rem;
  background-color: #282828;
  opacity: 0.88;
  font-family: 微软雅黑;
}
.hp_download .close {
  float: left;
  width: 1.25rem;
  height: 1.25rem;
  padding: 0.9375rem 0.1875rem;
  box-sizing: content-box;
}
.hp_download figure {
  margin: 0;
  padding-top: 0.3125rem;
  margin-left: 0.1875rem;
  float: left;
}
.hp_download figure img {
  float: left;
  width: 2.5rem;
  height: 2.5rem;
  border-radius: 0.3125rem;
}
.hp_download figure figcaption {
  float: left;
  margin-left: 0.5rem;
  margin-top: -0.25rem;
}
.hp_download figure figcaption p {
  color: #ff7800;
  font-size: 1.0625rem;
  letter-spacing: 0.03125rem;
  margin: 0.125rem 0 -0.125rem;
}
.hp_download figure figcaption span {
  font-size: 0.8125rem;
  color: #fff;
}
.hp_download .openApp {
  float: right;
  color: #fff;
  font-size: 1rem;
  background-color: #ff7800;
  border: none;
  padding: 0.4rem 1rem;
  margin-top: 0.45rem;
  margin-right: 0.5rem;
  text-decoration: none;
  border-radius: 0.3125rem;
}
