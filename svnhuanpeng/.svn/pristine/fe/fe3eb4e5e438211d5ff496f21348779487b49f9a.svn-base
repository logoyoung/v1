<script>
  function QueryLocation() {
    var b = location.href;
    var c = new Object();
    if (b.indexOf('?') > -1) {
      var str = b.substr(b.indexOf('?') + 1);
      var strs = str.split("&");
      for (var i = 0; i < strs.length; i++) {
        c[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
      }
    }
    return c;
  }
  QueryLocation();
  if (QueryLocation().code) {
    sessionStorage.setItem('_wxCode', QueryLocation().code);
  }

</script>

