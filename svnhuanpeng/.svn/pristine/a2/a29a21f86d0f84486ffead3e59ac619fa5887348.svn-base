@-webkit-keyframes run {
  from {
    width: 0;
    height: 0;
    transform: scale(0);
  }
  to {
    width: 3.125rem;
    height: 3.125rem;
    transform: scale(1);
  }
}
@-moz-keyframes run {
  from {
    width: 0;
    height: 0;
    transform: scale(0);
  }
  to {
    width: 3.125rem;
    height: 3.125rem;
    transform: scale(1);
  }
}
@keyframes run {
  from {
    width: 0;
    height: 0;
    transform: scale(0);
  }
  to {
    width: 3.125rem;
    height: 3.125rem;
    transform: scale(1);
  }
}
